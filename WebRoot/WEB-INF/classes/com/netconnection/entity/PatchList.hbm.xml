<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.netconnection.entity.PatchList" table="patchlist" catalog="netconnection">
        <id name="patchid" type="int">
            <column name="patchid" />
            <generator class="native" />
        </id>      
        <property name="patchname" type="java.lang.String">
            <column name="patchname" length="45" not-null="true" />
        </property> 
         <property name="choose" type="java.lang.String">
            <column name="choose" length="45" not-null="false" />
        </property> 
        
        <property name="degree" type="java.lang.String">
            <column name="degree" length="45" not-null="true" />
        </property> 
        <property name="os" type="java.lang.String">
            <column name="os" length="45" not-null="true" />
        </property> 
        <property name="description" type="java.lang.String">
            <column name="description" length="45" not-null="false" />
        </property> 
        
    </class>
</hibernate-mapping>
