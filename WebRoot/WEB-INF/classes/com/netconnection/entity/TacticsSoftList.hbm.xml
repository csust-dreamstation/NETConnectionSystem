<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.netconnection.entity.TacticsSoftList" table="tactics_soft" catalog="netconnection">
        <id name="id" type="int">
            <column name="Id" />
            <generator class="native" />
        </id>      
        <property name="tacticsid" type="java.lang.Integer">
            <column name="tacticsid" length="11" not-null="false" />
        </property> 
        <property name="listid" type="java.lang.Integer">
            <column name="listid" length="11" not-null="false" />
        </property>
        <property name="statu" type="java.lang.Integer">
            <column name="statu" length="11" not-null="false" />
        </property>
    </class>
</hibernate-mapping>
