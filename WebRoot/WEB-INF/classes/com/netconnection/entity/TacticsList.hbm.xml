<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.netconnection.entity.TacticsList" table="tacticslist" catalog="netconnection">
        <id name="tacticsid" type="int">
            <column name="tacticsid" />
            <generator class="native" />
        </id>      
        <property name="tacticsname" type="java.lang.String">
            <column name="tacticsname" length="45" not-null="true" />
        </property> 
        
        <property name="blacktime" type="java.lang.Integer">
            <column name="blacktime" length="11" not-null="false" />
        </property> 
        
        <property name="whitetime" type="java.lang.Integer">
            <column name="whitetime" length="11" not-null="false" />
        </property> 
        
        <property name="patchtime" type="java.lang.Integer">
            <column name="patchtime" length="11" not-null="false" />
        </property> 
        
    </class>
</hibernate-mapping>
